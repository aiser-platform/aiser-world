{
  "name": "aiser-world",
  "version": "1.0.0",
  "description": "AI-powered alternative to BI Tools with open source core chat2chart",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces",
    "dev:chat2chart": "npm run dev --workspace=packages/chat2chart/client",
    "dev:client": "npm run dev --workspace=packages/client/client",
    "build": "npm run build --workspaces",
    "build:chat2chart": "npm run build --workspace=packages/chat2chart/client",
    "build:client": "npm run build --workspace=packages/client/client",
    "test": "npm run test --workspaces",
    "test:unit": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "npm run lint --workspaces",
    "lint:fix": "npm run lint:fix --workspaces",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "npm run clean --workspaces && rm -rf node_modules",
    "postinstall": "npm run setup:shared",
    "setup:shared": "node tools/setup-shared.js",
    "license:check": "node tools/license-checker.js",
    "docs:dev": "npm run start --workspace=packages/docs -- --port 3001",
    "docs:build": "npm run build --workspace=packages/docs",
    "docs:generate": "npm run generate-docs --workspace=packages/docs",
    "docs:deploy": "npm run deploy --workspace=packages/docs"
  },
  "keywords": [
    "ai",
    "analytics",
    "powerbi",
    "charts",
    "visualization",
    "open-source"
  ],
  "author": "DataTicon Team",
