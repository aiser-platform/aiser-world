{
  "pythonVersion": "3.11",
  "venvPath": "/workspace",
  "venv": ".venv311",
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "reportMissingTypeStubs": "none",
  "reportMissingImports": "warning",
  "reportUnknownVariableType": "warning",
  "reportUnknownMemberType": "warning",
  "reportUnknownArgumentType": "warning",
  "include": [
    "packages/chat2chart/server/app",
    "packages/client/server/app"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/.git/**",
    "**/.venv*/**",
    ".venv*/**",
    "**/__pycache__/**",
    "packages/**/client/**",
    "packages/**/docs/**",
    "packages/docs/**",
    "packages/**/tests/**",
    "packages/**/migrations/**",
    "packages/**/venv/**",
    "packages/**/build/**",
    "packages/**/dist/**",
    "packages/**/coverage/**",
    "packages/**/e2e/**",
    "packages/**/scripts/**",
    "packages/monitoring-service/**",
    "packages/rate-limiting-service/**",
    "packages/auth-service/**",
    "packages/billing-service/**",
    "packages/backup-service/**",
    "packages/chat2chart/server/app/modules/projects/**",
    "packages/chat2chart/server/app/modules/teams/**",
    "packages/chat2chart/server/app/modules/chats/**",
    "packages/chat2chart/server/app/modules/cube/**",
    "packages/chat2chart/server/app/modules/data/**",
    "packages/auth/**",
    "scripts/**",
    "temp-test/**",
    "tmp_*.py"
  ],
  "reportMissingImports": "none",
  "reportMissingModuleSource": "none",
  "reportUnknownVariableType": "none",
  "reportUnknownMemberType": "none",
  "reportUnknownArgumentType": "none",
  "executionEnvironments": [
    {
      "root": "packages/chat2chart/server",
      "extraPaths": ["packages/chat2chart/server"]
    },
    {
      "root": "packages/client/server",
      "extraPaths": ["packages/client/server"]
    },
    {
      "root": ".",
      "extraPaths": [
        "packages/chat2chart/server",
        "packages/client/server"
      ]
    }
  ]
}

